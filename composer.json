{
    "name": "serh/observer",
    "description": "Observer and composer test example",
    "keywords": ["observer", "test"],
    "license": "MIT",
    
    "authors": [
        {
            "name": "Serhii Nikolaichuk",
            "email": "serh@limit.com.ua"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Serh\\Observer\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    },
    "scripts": {
        "test": [
            "parallel-lint . --exclude vendor",
            "phpunit ./tests/*.php"
        ]
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "dev-master",
        "phpunit/phpunit": "7.5.x-dev"
    }
}
